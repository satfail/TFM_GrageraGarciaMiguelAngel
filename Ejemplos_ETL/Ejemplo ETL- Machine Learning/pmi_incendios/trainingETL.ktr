    <py_script># python script

import pandas as pd
import numpy as np
import matplotlib.pyplot as plt

def mapIndiceTotal(indiceTotal):
    categorical =""
    if(indiceTotal &lt; 24):
        categorical = "Leve"
    if(indiceTotal >= 24 and indiceTotal &lt; 49 ):
        categorical = "Moderado"
    if(indiceTotal >= 49 and indiceTotal &lt; 74 ):
        categorical = "Alto"
    if(indiceTotal >=  74 ):
        categorical = "Extremo"
    return categorical


def mapIndiceiquidPrecipitation(sumcumDaysPrep):
    indiceiquidPrecipitation = 0
    sumcumInt = int(sumcumDaysPrep)
    if(sumcumInt == 1):
        indiceiquidPrecipitation = 3.5
    if(sumcumInt > 1 and sumcumInt &lt;= 4 ):
        indiceiquidPrecipitation = 7
    if(sumcumInt > 5 and sumcumInt &lt;= 7 ):
        indiceiquidPrecipitation = 10.5
    if(sumcumInt > 8 and sumcumInt &lt;= 10 ):
        indiceiquidPrecipitation = 14
    if(sumcumInt > 11 and sumcumInt &lt;= 13 ):
        indiceiquidPrecipitation = 17.5
    if(sumcumInt > 14 and sumcumInt &lt;= 16 ):
        indiceiquidPrecipitation = 21
    if(sumcumInt > 17 and sumcumInt &lt;= 19 ):
        indiceiquidPrecipitation = 24.5
    if(sumcumInt > 20 and sumcumInt &lt;= 22 ):
        indiceiquidPrecipitation = 28
    if(sumcumInt > 23 and sumcumInt &lt;= 25 ):
        indiceiquidPrecipitation = 31.5
    if(sumcumInt > 26):
        indiceiquidPrecipitation = 35
    return indiceiquidPrecipitation

dataset["sumDay"] = pd.to_numeric(dataset["sumDay"])
dataset["indiceSum"] = pd.to_numeric(dataset["indiceSum"])



dataset["sumcumDaysPrep"] = dataset["sumDay"].groupby(((dataset["sumDay"]  != dataset["sumDay"].shift())).cumsum()).cumcount() 



dataset.loc[(dataset["sumcumDaysPrep"] > 0) &amp; (dataset["sumDay"] > 0), "sumcumDaysPrep"] = 0


dataset["indicePrec"] = dataset["sumcumDaysPrep"].map(lambda x : mapIndiceiquidPrecipitation(x))
dataset["indiceTotal"] = dataset["indicePrec"] + dataset["indiceSum"]

dataset["categorical"] = dataset["indiceTotal"].map(lambda x : mapIndiceTotal(x))


filteredDataSet = dataset.loc[:,["humidity","temperature","windSpeed","sumcumDaysPrep","categorical"] ]





</py_script>
