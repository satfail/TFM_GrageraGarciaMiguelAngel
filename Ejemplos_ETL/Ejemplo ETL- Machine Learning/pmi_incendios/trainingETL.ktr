    <connection>Plataforma</connection>
    <sql>SELECT 
	DATE_FORMAT(dateModified, '%Y-%m-%d') as dateModified,	
	DAY(dateModified) as dayModified,
	MONTH(dateModified) as monthModified,
	YEAR(dateModified) as yearModified,
	entityId ,
	latitude ,
	longitude ,
	entityType,
	cast(humidity as DECIMAL(12,2)) as humidity ,
	CASE 
		WHEN cast( humidity as DECIMAL  ) >= 80 then 2.5 	
		WHEN cast( humidity as DECIMAL  ) >= 75 and cast( humidity as DECIMAL  )  &lt; 80 then 5 
		WHEN cast( humidity as DECIMAL  ) >= 70 and cast( humidity as DECIMAL  )  &lt; 75 then 7.5 
		WHEN cast( humidity as DECIMAL  ) >= 65 and cast( humidity as DECIMAL  )  &lt; 70 then 10 
		WHEN cast( humidity as DECIMAL  ) >= 60 and cast( humidity as DECIMAL  )  &lt; 65 then 12.5 
		WHEN cast( humidity as DECIMAL  ) >= 55 and cast( humidity as DECIMAL  )  &lt; 60 then 15 
		WHEN cast( humidity as DECIMAL  ) >= 50 and cast( humidity as DECIMAL  )  &lt; 55 then 17.5
		WHEN cast( humidity as DECIMAL  ) >= 45 and cast( humidity as DECIMAL  )  &lt; 50 then 20
		WHEN cast( humidity as DECIMAL  ) >= 40 and cast( humidity as DECIMAL  )  &lt; 45 then 22.5
		WHEN cast( humidity as DECIMAL  ) &lt; 40 then 25
	end as indiceHumidity,
	cast(temperature as DECIMAL(12,2)) as temperature,
	CASE 
		WHEN cast( temperature as DECIMAL  ) &lt; 10 then 2.5 
		WHEN cast( temperature as DECIMAL  ) >= 10  and cast( temperature as DECIMAL  ) &lt; 12  then 5
		WHEN cast( temperature as DECIMAL  ) >= 12  and cast( temperature as DECIMAL  ) &lt; 14  then 7.5
		WHEN cast( temperature as DECIMAL  ) >= 14  and cast( temperature as DECIMAL  ) &lt; 16  then 10
		WHEN cast( temperature as DECIMAL  ) >= 16  and cast( temperature as DECIMAL  ) &lt; 18  then 12.5
		WHEN cast( temperature as DECIMAL  ) >= 18  and cast( temperature as DECIMAL  ) &lt; 20  then 15
		WHEN cast( temperature as DECIMAL  ) >= 20  and cast( temperature as DECIMAL  ) &lt; 22  then 17.5
		WHEN cast( temperature as DECIMAL  ) >= 22  and cast( temperature as DECIMAL  ) &lt; 24  then 20
		WHEN cast( temperature as DECIMAL  ) >= 24  and cast( temperature as DECIMAL  ) &lt; 26  then 22.5
		WHEN cast( temperature as DECIMAL  ) >= 26 then 25 
	end as indiceTemperature  , 
	 cast(windSpeed as DECIMAL(12,2)) as windSpeed,
	CASE 
		WHEN cast( windSpeed as DECIMAL  ) &lt; 3 then 2.5 
		WHEN cast( windSpeed as DECIMAL  ) >= 3  and cast( windSpeed as DECIMAL  ) &lt; 6  then 5
		WHEN cast( windSpeed as DECIMAL  ) >= 6  and cast( windSpeed as DECIMAL  ) &lt; 9  then 7.5
		WHEN cast( windSpeed as DECIMAL  ) >= 9  and cast( windSpeed as DECIMAL  ) &lt; 12  then 10
		WHEN cast( windSpeed as DECIMAL  ) >= 12  and cast( windSpeed as DECIMAL  ) &lt; 15  then 12.5
		WHEN cast( windSpeed as DECIMAL  ) >= 15  and cast( windSpeed as DECIMAL  ) &lt; 18  then 15
		WHEN cast( windSpeed as DECIMAL  ) >= 18  and cast( windSpeed as DECIMAL  ) &lt; 21  then 17.5
		WHEN cast( windSpeed as DECIMAL  ) >= 21  and cast( windSpeed as DECIMAL  ) &lt; 24  then 20
		WHEN cast( windSpeed as DECIMAL  ) >= 24  and cast( windSpeed as DECIMAL  ) &lt; 27  then 22.5
		WHEN cast( windSpeed as DECIMAL  ) >= 27 then 25 
	end as indiceWindSpeed  , 
	liquidPrecipitation
from EAQR_AEMET_O eao
WHERE liquidPrecipitation not LIKE ' ';</sql>
