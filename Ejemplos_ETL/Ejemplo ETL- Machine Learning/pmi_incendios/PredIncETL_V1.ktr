    </partitioning>
    <connection>PlataformaRaw</connection>
    <sql>SELECT *
FROM
(SELECT 
	dateModified, 
	DATE_FORMAT(dateModified, '%Y-%m-%d') as dateModifiedFormat,	
	entityId ,
	CONCAT('{"type":"Point","coordinates":[', longitude, ',', latitude ,']}') as location,
	latitude ,
	longitude ,
	entityType,
	cast(humidity as DECIMAL(12,2)) as humidity ,
	cast(temperature as DECIMAL(12,2)) as temperature,
 	cast(windSpeed as DECIMAL(12,2)) as windSpeed,
 	cast(precipitation as DECIMAL(12,2)) as precipitation
from EAQR_AEMET_O ) as A
	join 
		(
		SELECT 
		entityId,
		DATE_FORMAT(dateModified, '%Y-%m-%d') as dateModifiedFormat,	
		sum(precipitation)
		FROM
			EAQR_AEMET_O 
		WHERE DATE_FORMAT(dateModified, '%Y-%m-%d') BETWEEN DATE_SUB(DATE_FORMAT(now(), '%Y-%m-%d'), INTERVAL 26 DAY) and DATE_FORMAT(now(), '%Y-%m-%d') 
		GROUP BY entityId, DATE_FORMAT(dateModified, '%Y-%m-%d')
		) B
	on A.dateModifiedFormat = B.dateModifiedFormat and A.entityId = B.entityId
order by 2 asc;</sql>
