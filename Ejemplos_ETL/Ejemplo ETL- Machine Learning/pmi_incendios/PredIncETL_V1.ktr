    <py_script># python script

import pandas as pd
import numpy as np


# Cambio dtypes
dataset["humidity"] = pd.to_numeric(dataset["humidity"])
dataset["temperature"] = pd.to_numeric(dataset["temperature"])
dataset["sumDayPrep"] = pd.to_numeric(dataset["sumDayPrep"])
dataset["windSpeed"] = pd.to_numeric(dataset["windSpeed"])

# Agrupo la suma de precipitaciones en cada dia y entidad
daysSumRainById = dataset.loc[:, ["entityId","sumDayPrep", "dateModifiedFormat"]].groupby(["dateModifiedFormat", "entityId"], as_index=False).sum()

#Ordeno para calcular dias consecutivos
daysSumRainById = daysSumRainById.sort_values(['entityId','dateModifiedFormat'],ascending=True)

# Por cada tipo de entidad cuenta los dias con el mismo valor
for item in daysSumRainById["entityId"].unique():
    boolCondition = daysSumRainById["entityId"] == item
    daysSumRainById.loc[boolCondition, "sumcumDaysPrec"] =  daysSumRainById.loc[boolCondition, "sumDayPrep"].groupby(((daysSumRainById["sumDayPrep"]  != daysSumRainById["sumDayPrep"].shift())).cumsum()).cumcount()

#Juntamos
dataset = pd.merge(dataset, daysSumRainById, on=['dateModifiedFormat','entityId'], how='inner',suffixes=('_Prev','_Final'))

#Eliminamos registros que tengan un valor de suma repetido en sus dias
dataset.loc[(dataset["sumcumDaysPrec"] > 0) &amp; (dataset["sumDayPrep_Prev"] > 0), "sumcumDaysPrec"] = 0

#Ordenamos para sacar valor actual
dataset = dataset.sort_values('dateModified', ascending=False)

#Sacamos las 21 entidades
datasetNow = dataset.head(21)

#Resteo de index y me llevo las columnas
datasetNow = datasetNow.reset_index()
datasetNowClean = datasetNow.loc[:,["dateModified","dateModifiedFormat","entityId","location",
                               "humidity","temperature","windSpeed","sumcumDaysPrec"]]


</py_script>
